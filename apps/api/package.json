{
  "type": "module",
  "name": "@proveo/tutoring-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "api:test": "nx test run",
    "api:test:watch": "nx test",
    "api:test:coverage": "nx test run --coverage",
    "api:test:integration": "./test/integration.sh",
    "api:start": "pnpm run api:build:ts && fastify start -l info dist/app.js",
    "api:build:ts": "tsc -p tsconfig.build.json",
    "api:watch:ts": "tsc -w",
    "api:dev": "pnpm run api:build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:api:watch:ts\" \"pnpm:api:dev:start\"",
    "api:dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": "^22"
  },
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/http-proxy": "^11.1.2",
    "@fastify/multipart": "^9.0.3",
    "@fastify/sensible": "^6.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "json-schema-to-ts": "^2.12.0",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/unzipper": "^0.10.11",
    "@vitest/coverage-v8": "^3.1.4",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.1.4"
  }
}
